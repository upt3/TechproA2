class Student {
    String name;

    void displayName() {
        if (name == null || name.isEmpty()) {
            System.out.println("Error: Student name is not set.");
        } else {
            System.out.println("Student Name: " + name);
        }
    }
}

public class Main {
    public static void main(String[] args) {
        try {
            
            Student s1 = new Student();

  
            Student s2 = s1;

            s2.name = "Shem Daniel";

            s1.displayName();
        } catch (Exception e) {
            System.out.println("An error occurred: " + e.getMessage());
        }
    }
